{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
  {
	"name": "fitnesse-content",
	"image": "zsuffazs/fitnesse-docker-content:latest",
	"essential": false,
	"memory": 128
  },
  
  {
    "name": "fitnesse-app",
    "image": "zsuffazs/fitnesse-docker-server:latest",
    "essential": true,
    "memory": 256,
    "volumesFrom": [
      {
        "sourceContainer": "fitnesse-content"
      }
    ],
    "portMappings": [
      {
        "hostPort": 9123,
        "containerPort": 9123
      }
    ],
    "command": [
      "mvn -f fitnesse-connect-connect/pom.xml clean verify -P wiki"
    ]
  }
]
}